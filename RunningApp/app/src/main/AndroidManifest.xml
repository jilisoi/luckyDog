<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.whitworth.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="edu.whitworth.app.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="edu.whitworth.app.Run"
            android:label="@string/title_activity_run" >
        </activity>
        <activity
            android:name="edu.whitworth.app.Results"
            android:label="@string/title_activity_results" >
        </activity>
        <activity
            android:name="edu.whitworth.app.Settings"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="edu.whitworth.app.HistoricalResults"
            android:label="@string/title_activity_historical_results" >
        </activity>
    </application>

</manifest>
